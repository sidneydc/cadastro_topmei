#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/mcpb/dist/cli/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/mcpb/dist/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/mcpb/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/mcpb/dist/cli/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/mcpb/dist/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/mcpb/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules/@anthropic-ai/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/@anthropic-ai+mcpb@1.1.1/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli/cli.js" "$@"
fi
