#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/.local/share/pnpm/global/5/.pnpm/nanoid@3.3.11/node_modules/nanoid/bin/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/nanoid@3.3.11/node_modules/nanoid/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/nanoid@3.3.11/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/.local/share/pnpm/global/5/.pnpm/nanoid@3.3.11/node_modules/nanoid/bin/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/nanoid@3.3.11/node_modules/nanoid/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/nanoid@3.3.11/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.3.11/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../nanoid@3.3.11/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
